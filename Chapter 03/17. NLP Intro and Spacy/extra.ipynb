    "def preprocess(pth):\n",
    "\n",
    "    with open(pth, 'r') as f:\n",
    "        lines = f.readlines()\n",
    "        for i in range(len(lines)):\n",
    "            lines[i] = lines[i].replace(\".\", \"\").replace(\",\",\"\").replace(\":\",\"\")        \n",
    "        return lines[i]\n",
    "\n",
    "preprocess(\"data\\\\article.txt\")\n",
    "\n",
    "\n"
   ]
